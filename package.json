{
    "name": "vscode-ldjs",
    "displayName": "vscode-ldjs",
    "description": "vscode adapter for ldjs",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ldjsstart",
        "onLanguage:js"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.ldjsstart",
                "title": "LDJS: Start Server"
            },
            {
                "command": "extension.ldjsend",
                "title": "LDJS: Stop Server"
            },
            {
                "command": "extension.ldjsclearrequire",
                "title": "LDJS: Clear require cache"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.6",
        "vscode": "^1.1.21",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "fp-ts": "^1.11.2",
        "lambdadesignerjs": "file:../LambdaDesignerJS",
        "net": "^1.0.2"
    }
}
